<h1>Daftar Laporan Pegawai Bimkat Sumteng</h1>

<table>
  <thead>
    <tr>
      <th>Judul</th>
      <th>Tautan</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @laporan_pegawai_bimkat_sumteng.each do |laporan_pegawai_bimkat_sumteng| %>
      <tr>
        <td><%= laporan_pegawai_bimkat_sumteng.judul %></td>
        <td><%= laporan_pegawai_bimkat_sumteng.tautan %></td>
        <td><%= link_to 'Tampilkan', laporan_pegawai_bimkat_sumteng %></td>
      <% if pengguna_signed_in? %>
        <% if @pengguna.guru_pendakat? || @pengguna.administrator? %>
        <td><%= link_to 'Edit', edit_laporan_pegawai_bimkat_sumteng_path(laporan_pegawai_bimkat_sumteng) %></td>
        <td><%= link_to 'Hapus', laporan_pegawai_bimkat_sumteng, method: :delete, data: { confirm: 'Anda yakin ingin menghapusnya?' } %></td>
        <% end %>
      <% end %>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'Membuat Laporan Pegawai Bimkat Sumteng', new_laporan_pegawai_bimkat_sumteng_path %>
