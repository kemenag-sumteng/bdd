<p id="notice"><%= notice %></p>

<h1>Laporan Pegawai Bimkat Sumteng</h1>

<table>
  <thead>
    <tr>
      <th>Judul</th>
      <th>Tautan</th>
      <th>Pengguna</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @laporan_pegawai_bimkat_sumteng.each do |laporan_pegawai_bimkat_sumteng| %>
      <tr>
        <td><%= laporan_pegawai_bimkat_sumteng.judul %></td>
        <td><%= laporan_pegawai_bimkat_sumteng.tautan %></td>
        <td><%= laporan_pegawai_bimkat_sumteng.pengguna %></td>
        <td><%= link_to 'Show', laporan_pegawai_bimkat_sumteng %></td>
        <td><%= link_to 'Edit', edit_laporan_pegawai_bimkat_sumteng_path(laporan_pegawai_bimkat_sumteng) %></td>
        <td><%= link_to 'Destroy', laporan_pegawai_bimkat_sumteng, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

