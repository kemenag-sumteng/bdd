<h1>Daftar Laporan Penyuluh Agama Katolik</h1>

<table>
  <thead>
    <tr>
      <th>Judul</th>
      <th>Tautan</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @laporan_penyuluh_agama_katolik.each do |laporan_penyuluh_agama_katolik| %>
      <tr>
        <td><%= laporan_penyuluh_agama_katolik.judul %></td>
        <td><%= laporan_penyuluh_agama_katolik.tautan %></td>
        <td><%= link_to 'Tampilkan', laporan_penyuluh_agama_katolik %></td>
        <td><%= link_to 'Edit', edit_laporan_penyuluh_agama_katolik_path(laporan_penyuluh_agama_katolik) %></td>
        <td><%= link_to 'Hapus', laporan_penyuluh_agama_katolik, method: :delete, data: { confirm: 'Anda yakin ingin menghapusnya?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'Membuat Laporan Penyuluh Agama Katolik', new_laporan_penyuluh_agama_katolik_path %>
